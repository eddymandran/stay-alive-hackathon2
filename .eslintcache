edans !!!', {\n        appearance: 'error',\n        autoDismiss: true,\n      });\n    }\n  }\n\n  const [users, setUsers] = useState([]);\n\n  useEffect(() => {\n    const getUsers = fb\n      .firestore()\n      .collection(\"Users\")\n      .orderBy(\"name\", \"asc\")\n      .onSnapshot((u) => {\n        setUsers(\n          u.docs.map((user) => {\n            return { id: user.id, ...user.data() };\n          })\n        );\n      });\n\n    return () => getUsers();\n  }, []);\n\n  useEffect(() => {\n    goodPlayer();\n  }, [scorePlayer]);\n\n  return (\n    <div className='scoreBar'>\n      <div className='zoneAvatar'>\n        <img alt='personnage choisi'></img>\n        <h5 className='title5'>NickName :</h5>\n      </div>\n      <hr />\n      <div className='zoneScore'>\n        <h5 className='title5'> Your score :<p>{scorePlayer}</p></h5>\n        <img alt='schtroumph'></img>\n      </div>\n      <hr />\n      <div className='zoneCharacters'>\n        <h5 className='title5'>Fugitive(s)</h5>\n        <ul className='charactersList'>\n        {users.map((u) => {\n            return (\n              <li key={u.id}>\n                {u.name} \n              </li>\n            );\n          })}\n        </ul>\n      </div>\n    </div>\n  );\n}\n","/home/autier/Documents/wildcode/Hackathon/stay-alive-hackathon2/src/Components/Board.js",["56"],"/home/autier/Documents/wildcode/Hackathon/stay-alive-hackathon2/src/services/firebase.js",[],{"ruleId":"57","replacedBy":"58"},{"ruleId":"59","replacedBy":"60"},{"ruleId":"61","severity":1,"message":"62","line":11,"column":10,"nodeType":"63","messageId":"64","endLine":11,"endColumn":15},{"ruleId":"61","severity":1,"message":"65","line":11,"column":17,"nodeType":"63","messageId":"64","endLine":11,"endColumn":25},{"ruleId":"61","severity":1,"message":"66","line":12,"column":10,"nodeType":"63","messageId":"64","endLine":12,"endColumn":27},{"ruleId":"61","severity":1,"message":"67","line":12,"column":29,"nodeType":"63","messageI